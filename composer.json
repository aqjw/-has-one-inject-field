{
    "name": "aqjw/nova-has-one-inject-field",
    "description": "Inject HasOne field in the form between other fields.",
    "keywords": [
        "laravel",
        "nova",
        "HasOne",
        "inject"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Aqjw\\HasOneInjectField\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Aqjw\\HasOneInjectField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
